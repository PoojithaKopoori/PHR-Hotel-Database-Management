    <title>Hello, world!</title>
  </head>
  <body>
    <section class="h-100 h-custom" style="background-color: #8fc4b7; ">
  <div class="container py-5 h-100">
    <div class="row d-flex justify-content-center align-items-center h-100">
      <div class="col-lg-8 col-xl-6">
        <div class="card rounded-3">
          <img src="https://images.unsplash.com/photo-1590381105924-c72589b9ef3f?ixlib=rb-4.0.3&ixid=MnwxMjA3fDB8MHxzZWFyY2h8NzF8fGhvdGVsfGVufDB8fDB8fA%3D%3D&auto=format&fit=crop&w=800&q=60" height="200px"
            class="w-100" style="border-top-left-radius: .3rem; border-top-right-radius: .3rem;"
            alt="Sample photo">
          <div class="card-body p-4 p-md-5">
            <h3 class="mb-4 pb-2 pb-md-0 mb-md-5 px-md-2">Registration Info</h3>

            <form action="/authregister/registrationpage" method="post" class="px-md-2">

              <div class="form-outline mb-4">
                <label class="form-label" for="form3Example1q"></label>
                <input type="text" id="form3Example1q" class="form-control" name="name" placeholder="Name"/>

              </div>
              <div class="mb-3">
            <label class="form-label" for="inputEmail"></label>
            <input type="email" class="form-control is-valid" id="inputEmail" placeholder="Email" name="email" required>
            <div class="valid-feedback">Good! Your email address looks valid.</div>
        </div>
        <br>
              <div class="form-outline mb-3">
                <input type="password" id="typePasswordX-2" class="form-control form-control-lg " name="password" placeholder="Password" />
                <label class="form-label" for="typePasswordX-2"></label>
              </div>
              <center>
              <div style="padding-bottom: 18px;"><input class="btn btn-outline-success" name="skip_Submit" value="Submit" type="submit"/></div>
              <div></center>

            </form>

          </div>
        </div>
      </div>
    </div>
  </div>
</section>

    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
