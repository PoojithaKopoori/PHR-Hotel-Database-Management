    <center>
      <div class="card" style="width: 900px; box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19); background-color: #DAEAF1; color:black;">
  <div class="card-body">
    <h3 style="font-family: Playfair Display;"><class="card-title">Customer Details</h3>
    <br><br>
    <% if(typeof name!="undefined"&& name){%>

        <span>
          <h6 style="display: inline; font-family: roboto;"><% if(data.length){ for(var i=0;i < data.length ; i++){%>
            <table >
              <tr>
                <td> HOTEL:</td>
                <td><%= data[i].hotel %>&nbsp;</td>
              </tr>
              <tr>
                <td>Room Selected : </td>
                <td><%= data[i].room_selected %>&nbsp;</td>
              </tr>
              <tr>
                <td>First Name : </td>
                <td><%= data[i].first_name %>&nbsp;</td>
              </tr>
              <tr>
                <td>Lat Name : </td>
                <td><%= data[i].last_name%>&nbsp;</td>
              </tr>
              <tr>
                <td>Contact Number : </td>
                <td><%= data[i].contact_no %>&nbsp;</td>
              </tr>
              <tr>
                <td>Email : </td>
                <td><%= data[i].email %>&nbsp;</td>
              </tr>
              <tr>
                <td>Arrival Date : </td>
                <td><%= data[i].arrival_date %>&nbsp;</td>
              </tr>
              <tr>
                <td>Departure Date : </td>
                <td><%= data[i].departure_date %>&nbsp;</td>
              </tr>
              <tr>
                <td>Number of Adults : </td>
                <td><%= data[i].number_of_adults %>&nbsp;</td>
              </tr>
              <tr>
                <td>Number of Children : </td>
                <td><%= data[i].number_of_children %>&nbsp;</td>
              </tr>
              <tr>
                <td>Selected Room : </td>
                <td><%= data[i].selected_room %>&nbsp;</td>
              </tr>
              <tr>
                <td>Total Amount : </td>
                <td><%= data[i].total_amount %>&nbsp;</td>
              </tr>
              <tr>
                <td>Status : </td>
                <td><%= data[i].status %>&nbsp;</td>
              </tr>

            </table>

            <% } %>
            <% } %></h6>
        </span>


      <%}%>
    <!-- <a href="#" class="btn btn-primary">Go somewhere</a> -->
  </div>
</div>

    </center>

