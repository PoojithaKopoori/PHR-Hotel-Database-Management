      <span class="navbar-toggler-icon"></span>
    </button>

    <div style="padding-left: 350px; border-width:0px; outline-width:0px;" class="collapse navbar-collapse" id="navbarSupportedContent">
      <ul class="navbar-nav mr-auto">
        <li class="nav-item active">
          <a class="nav-link" href="/index">Home <span class="sr-only">(current)</span></a>
        </li>
        <li class="nav-item">
          <a class="nav-link active" href="/dining">Dining</a>
        </li>
        <li class="nav-item">
          <a class="nav-link active" href="/room">Rooms</a>
        </li>
        <li class="nav-item">
          <a class="nav-link active" href="/transport">Transport</a>
        </li>
        <li class="nav-item">
          <a class="nav-link active" href="/contactus">Contact Us</a>
        </li>
        <li class="nav-item dropdown">
          <a class="nav-link dropdown-toggle active" href="/gallery" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
            Gallery
          </a>
          <div class="dropdown-menu " aria-labelledby="navbarDropdown">
            <a class="dropdown-item " href="/gallery">Gym</a>
            <a class="dropdown-item " href="/gallery">Swimming Pool</a>
            <a class="dropdown-item " href="/gallery">Spa with Steam & Sauna</a>
            <a class="dropdown-item " href="/gallery">Garden View</a>
            <a class="dropdown-item " href="/gallery">Indoor games</a>
            <a class="dropdown-item " href="/gallery">Sunset View</a>
        </li>
      </ul>
      <a  class="btn btn-outline-secondary btn-sm" href="/index" role="button"><h4>Logout</h4></a>
    </div>
  </nav>
    </div>
<br><br><br><br><br>
